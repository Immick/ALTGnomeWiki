{
  "format": "plain",
  "checkYo": true,
  "fileExtensions": [".md"],
  "excludeFiles": ["docs/equipment/hardware-compatibility.md"],
  "lang": "ru,en",
  "ignoreText": [
    ["```.+```", "gsm"],
    ["^  *", "gsm"],
    ["\\- src:.*$", "gsm"],
    " ?`.+`",
    "`.+` ?",
    "#+ ",
    "\\- +",
    "\\*\\*.*\\*\\* ?",
    " ?\\*\\*.*\\*\\*",
    "!?\\[.+](.+)",
    "<!-- ?@include.*-->",
    "<Badge .*>",
    "https?:\/\/([^ ]+) ",
    "{{.*}}",
    "\\| +"
  ]
}
